{
    "parser": "babel-eslint",
    "plugins": [
        "html", "react", "babel"
    ],
    "rules": {
        "no-console": 1,
        "react/jsx-uses-react": 2,
        "react/jsx-uses-vars": 2,
        "react/react-in-jsx-scope": 2
    },
    "env": {
        "es6": true,
        "node": true,
        "browser": true
    },
    "extends": "eslint:recommended"
}
